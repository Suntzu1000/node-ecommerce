{
  "version": 2,
  "name": "node_ecommerce",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/index.js", 
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      } 
    }
  ]
}



